@using MixERP.Net.UI.ScrudFactory
@model Config

@Html.ScrudFor(Model)

<script type="text/javascript">
    function scrudCustomValidator() {
        var baseUnitId = $("#base_unit_id");
        var compareUnitId = $("#compare_unit_id");

        var baseUnit = parseInt2(baseUnitId.getSelectedValue());
        var compareUnit = parseInt2(compareUnitId.getSelectedValue());

        if (baseUnit === compareUnit) {
            displayMessage("@ViewData["CompoundUnitOfMeasureErrorMessage"]");
            return false;
        };
        return true;
    };
</script>