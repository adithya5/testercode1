@using MixERP.Net.UI.ScrudFactory
@model Config

@Html.ScrudFor(Model)


<script type="text/javascript">
    function scrudCustomValidator() {
        var startFromTextbox = $("#starts_from");
        var endsOnTextbox = $("#ends_on");

        var startForm = parseDate(startFromTextbox.val());
        var endsOn = parseDate(endsOnTextbox.val());

        if (endsOn < startForm) {
            displayMessage("@ViewData["DateErrorMessage"]");

            return false;
        };
        return true;
    };
</script>
